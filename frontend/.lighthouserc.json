{
    "ci": {
        "collect": {
            "startServerCommand": "npm run preview",
            "url": [
                "http://localhost:4173/"
            ],
            "numberOfRuns": 3
        },
        "assert": {
            "assertions": {
                "largest-contentful-paint": [
                    "error",
                    {
                        "maxNumericValue": 3000
                    }
                ]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}